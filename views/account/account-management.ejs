<% if (title) { %>
<h1><%= title %></h1>
<% } else {
  res.redirect('/')
} %>

<%- messages() %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>


<h2>Welcom <%= accountData.account_firstname %></h2>

<a href="/account/update">Edit Account Information</a>

<% if (accountData.account_type === 'Admin' || accountData.account_type === 'Employee') { %>
  <h3>Inventory Management</h3>
  <a href="/inv">Manage Inventory</a>
<% } else{ %>

<% } %>